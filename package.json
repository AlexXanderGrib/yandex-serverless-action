  "dependencies": {
    "@actions/core": "^1.2.4",
    "archiver": "^4.0.1",
    "bl": "^4.0.2",
    "grpc": "^1.24.3",
    "yandex-cloud": "^1.3.3"
  },
  "devDependencies": {
    "@types/bl": "^2.1.0",
    "@types/archiver": "^3.1.0",
